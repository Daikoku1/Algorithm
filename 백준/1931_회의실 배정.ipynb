   "source": [
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "T = int(input())\n",
    "arr = [list(map(int, input().split())) for _ in range(T)]\n",
    "arr.sort(key = lambda x : (x[1],x[0]))\n",
    "\n",
    "last, cnt = 0, 0\n",
    "for i, j in arr:\n",
    "    if i >= last:\n",
    "        cnt += 1\n",
    "        last = j\n",
    "print(cnt)"
   ]
  }
