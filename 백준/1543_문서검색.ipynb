    "# 29200 KB, 72 ms\n",
    "s = input()\n",
    "t = input()\n",
    "idx, ans = 0, 0\n",
    "while idx <= len(s) - len(t):\n",
    "    find = True\n",
    "    for i in range(len(t)):\n",
    "        if s[idx + i] != t[i] :\n",
    "            find = False\n",
    "    if find : \n",
    "        ans += 1\n",
    "        idx += len(t)\n",
    "    else: idx += 1\n",
    "print(ans)"
   ]
  }
