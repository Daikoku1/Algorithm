   "source": [
    "# 29200 KB, 72 ms\n",
    "def BOJ_2231(N):\n",
    "    result = 0\n",
    "    min_num = max(1, N - 9*(len(str(N))))\n",
    "    for i in range(min_num, N):\n",
    "        result = i + sum(map(int, str(i)))\n",
    "        if result == N : return i\n",
    "    \n",
    "    return 0\n",
    "\n",
    "N = int(input())\n",
    "print(BOJ_2231(N))"
   ]
  }
