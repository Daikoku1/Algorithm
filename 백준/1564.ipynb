    "# 30860 KB, 556 ms\n",
    "N = int(input())\n",
    "ans = 1\n",
    "for i in range(2, N+1):\n",
    "    ans *= i\n",
    "    while True:\n",
    "        q, r = divmod(ans, 10)\n",
    "        if r == 0 :\n",
    "            ans = q\n",
    "        else:\n",
    "            break\n",
    "    ans %= 100000000000000000\n",
    "print(str(ans)[-5:])"
   ]
