    "# Time = 540 ms\n",
    "# Memory = 54796 KB\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "T = int(input())\n",
    "arr = sorted([int(input()) for _ in range(T)])\n",
    "ans = 0\n",
    "for idx, val in enumerate(arr, 1):\n",
    "    ans += abs(val - idx)\n",
    "print(ans)"
