    "# Time = 96 ms\n",
    "# Memory = 28776 KB\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "N = int(input())\n",
    "mod = 9901\n",
    "x, l, r = 1, 0, 0\n",
    "for _ in range(N):\n",
    "    x = (x + l + r) % mod\n",
    "    l, r = x - r, x - l\n",
    "print((x + l + r)%mod)"
   ]
