    "# Time = 76 ms\n",
    "# Memory = 29088 KB\n",
    "N = int(input())\n",
    "q, r = divmod(N, 5)\n",
    "while q != -1:\n",
    "    if r % 3 == 0:\n",
    "        print(q + r//3)\n",
    "        break\n",
    "    else:\n",
    "        q -= 1\n",
    "        r += 5\n",
    "else:print(-1)"
