    "import sys\n",
    "input = sys.stdin.readline\n",
    "D, H, M = map(int, input().split())\n",
    "if D == 11 and H * 60 + M < 11 * 60 + 11:print(-1)\n",
    "else:\n",
    "    print((D-11)*24*60 + (H-11)*60 +M - 11)"
   ]
