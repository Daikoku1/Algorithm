   "source": [
    "# 36676 KB, 124 ms\n",
    "N = int(input())\n",
    "answer = float('-inf')\n",
    "continue_sum = 0\n",
    "for x in map(int, input().split()):\n",
    "    continue_sum += x\n",
    "    # print(continue_sum)\n",
    "    answer = max(answer, continue_sum)\n",
    "    if continue_sum > 0 : continue\n",
    "    else: continue_sum = 0\n",
    "\n",
    "print(answer)"
   ]
  }
