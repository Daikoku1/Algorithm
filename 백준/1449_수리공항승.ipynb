    "# Time = 72 ms\n",
    "# Memory = 29088 KB\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "n, l = map(int, input().split())\n",
    "li = list(map(int, input().split()))\n",
    "prev, cnt = 0, 0\n",
    "for x in li:\n",
    "    if x <= prev:\n",
    "        continue\n",
    "    else:\n",
    "        prev = x + l -1\n",
    "        cnt += 1\n",
    "print(cnt)"
   ]
  }
