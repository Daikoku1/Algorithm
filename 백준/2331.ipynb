   "source": [
    "# 30864\tKB, 72 ms\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "a, p = map(int, input().split()) \n",
    "\n",
    "nums = [a]\n",
    "while True:\n",
    "    temp = 0\n",
    "    for s in str(nums[-1]):\n",
    "        temp += int(s) ** p\n",
    "    if temp in nums:\n",
    "        break\n",
    "    \n",
    "    nums.append(temp)\n",
    "\n",
    "print(nums.index(temp))"
   ]
  }
