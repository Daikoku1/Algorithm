    "import sys\n",
    "input = sys.stdin.readline\n",
    "T = int(input())\n",
    "for _ in range(T):\n",
    "    money = int(input())\n",
    "    q1, r1 = divmod(money, 25)\n",
    "    q2, r2 = divmod(r1, 10)\n",
    "    q3, r3 = divmod(r2, 5)\n",
    "    print(q1,q2,q3,r3)"
