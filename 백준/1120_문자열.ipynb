    "# 29200 KB, 72 ms\n",
    "A, B= map(str.strip, input().split())\n",
    "ans = len(A)\n",
    "for idx in range(len(B) - len(A)+1):\n",
    "    n_same = 0\n",
    "    for jdx, val in enumerate(A):\n",
    "        if val != B[idx+ jdx]: n_same += 1\n",
    "    ans = min(n_same, ans)\n",
    "\n",
    "print(ans)"
   ]
  }
