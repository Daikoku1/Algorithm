    "import sys\n",
    "input = sys.stdin.readline\n",
    "a, b, c = map(int, input().split())\n",
    "\n",
    "# a + b * x < c *x\n",
    "# x = a / (c-b)\n",
    "if c == b: print(-1)\n",
    "else:\n",
    "    q, r = divmod(a, c-b)\n",
    "    if q >= 0 : print(q + 1)\n",
    "    else: print(-1)"
   ]
