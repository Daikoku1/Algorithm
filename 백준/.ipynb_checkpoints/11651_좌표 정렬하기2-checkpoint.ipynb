    "# 51272 KB, 372 ms\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "N = int(input())\n",
    "ans = []\n",
    "for _ in range(N):\n",
    "    ans.append(tuple(map(int, input().split())))\n",
    "\n",
    "for x, y in sorted(ans, key = lambda x : (x[1], x[0])):\n",
    "    print(x,y)"
