    "# TIme = 68 ms\n",
    "# Memory = 28776 KB\n",
    "T = int(input())\n",
    "li = []\n",
    "for _ in range(T):\n",
    "    s = input()\n",
    "    num = 0\n",
    "    for c in s:\n",
    "        if c.isdigit() : num += int(c)\n",
    "    li.append([num , s])\n",
    "li.sort(key = lambda x : (len(x[1]), x[0],x[1]))\n",
    "for a, b in li:\n",
    "    print(b)"
   ]
  }
