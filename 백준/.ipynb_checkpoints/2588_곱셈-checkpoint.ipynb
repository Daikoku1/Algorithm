   "source": [
    "# 29200 KB,\t76 ms\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "n1 = int(input())\n",
    "n2 = input().strip()\n",
    "ans = 0\n",
    "for i, x in enumerate(n2[::-1]):\n",
    "    x = int(x)\n",
    "    sub = x * n1\n",
    "    print(sub)\n",
    "    ans += sub * 10**i\n",
    "print(ans)"
   ]
  }
