   "source": [
    "# 29200\tKB, 76 ms\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "for _ in range(3):\n",
    "    h1,m1,s1,h2,m2,s2 = map(int, input().split())\n",
    "    time = 3600*(h2-h1) + 60*m2 - 60*m1 + s2 - s1\n",
    "    time, s = divmod(time, 60)\n",
    "    h, m = divmod(time, 60)\n",
    "    print(h, m, s)"
   ]
  }
