        "def solution(array, commands):\n",
        "    answer = []\n",
        "    for pattern in commands:\n",
        "        i, j, k = pattern\n",
        "        answer.append(sorted(array[i-1:j])[k-1])\n",
        "        \n",
        "    return answer"
      ],
