    "def solution(n, results):\n",
    "    dp = [[0] *n for _ in range(n)]\n",
    "    for x, y in results:\n",
    "        dp[x-1][y-1] = 1\n",
    "        dp[y-1][x-1] = -1\n",
    "    return dp"
   ]
  },
