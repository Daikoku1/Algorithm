   "source": [
    "# Max_time = 0.03 ms\n",
    "# Momory = 10.3 MB\n",
    "def make_base(n):\n",
    "    q, r = divmod(n, 3)\n",
    "    r = str(r)\n",
    "    \n",
    "    if q == 0:\n",
    "        return r\n",
    "    else:\n",
    "        return make_base(q) + r\n",
    "def solution(n):\n",
    "    return int(make_base(n)[::-1], 3)"
   ]
